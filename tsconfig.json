{
  "version": "1.8.0",
  "compilerOptions": {
    "declaration": true,
    "module": "CommonJS",
    "target": "es5",
    "noImplicitAny": true,
    "noLib": false,
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "exclude": [
    "node_modules"
  ]
}